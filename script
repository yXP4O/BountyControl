if not game:IsLoaded() then
    game.Loaded:Wait()
end

repeat wait(0.001) until workspace.Players:FindFirstChild(game:service"Players".LocalPlayer.Name)

local testMode = false
if testMode then
    local newIDS = {}
    for i,v in pairs(game:service"Players":GetChildren()) do
        table.insert(newIDS, v.UserId)
    end
    getgenv().BountyControl.alts = newIDS
end

local remotes = {
    "CHECKER_1",
    "CHECKER_2",
    "TeleportDetect",
    "OneMoreTime",
    "BreathingHAMON",
    "VirusCough",
}

local __namecall
__namecall = hookmetamethod(game, "__namecall", function(...)
    local args = {...}
    local method = getnamecallmethod()
    if (method == "FireServer" and args[1].Name == "MainEvent" and table.find(remotes, args[2])) then
        return
    end
    return __namecall(table.unpack(args))
end)

local functions = loadstring(game:HttpGet("https://raw.githubusercontent.com/yXP4O/BountyControl/main/module.lua"))()

local player = game:service"Players".LocalPlayer
local Players = game:service"Players"
local isHost = player.UserId == BountyControl["host"]
local MainEvent = game:service"ReplicatedStorage".MainEvent
local red = Color3.fromRGB(255,0,0)
local green = Color3.fromRGB(0,255,0)
local whitelistedBuyer = ""
local stopProcess = false
local crashModeEnabled = false

game:service"ReplicatedStorage".DefaultChatSystemChatEvents.OnMessageDoneFiltering.OnClientEvent:Connect(function(data) 
    local user = data.FromSpeaker
    local msg = data.Message

    if game:service"Players":FindFirstChild(user).UserId == BountyControl["host"] then
        if msg:lower() == ".dw" then
            whitelistedBuyer = ""
        elseif msg:lower() == ".stop" then
            stopProcess = true
            functions.tpHome()
        end

        msg = msg:split(" ")
        if msg[1]:lower() == ".w" then
            whitelistedBuyer = msg[2]
        elseif msg[1]:lower() == ".ca" then
            local myId = functions.fetchMyId(getgenv().BountyControl["alts"], player.UserId)
            local newBreak = functions.splitString(msg[2])
            if table.find(newBreak, myId) then
                if player.UserId == getgenv().BountyControl["host"] then
                    functions.crashHelper(true, getgenv().BountyControl["host"])
                else
                    functions.crashHelper(false, getgenv().BountyControl["host"])
                end
            end
        elseif msg[1]:lower() == ".t" then
            local myId = functions.fetchMyId(getgenv().BountyControl["alts"], player.UserId)
            local newBreak = functions.splitString(msg[2])
            if table.find(newBreak, myId) then
                local buyerChar = game:service"Players":FindFirstChild(msg[3])
                if buyerChar.Character then
                    player.Character.HumanoidRootPart.CFrame = buyerChar.Character.HumanoidRootPart.CFrame * CFrame.new(0,2,0)
                    functions.say("ko & arrest me.")
                end
            end
        end
    end 
end)

player.CharacterAdded:Connect(function()
    repeat wait(0.01) until player.Character and player.Character:FindFirstChildWhichIsA("Humanoid").Health >= 80
    functions.tpHome()
end)

task.spawn(function()
    functions.tpHome()
    functions.blockScreen()
    functions.lowGFX(BountyControl['fps'])

    while true do wait(0.01)
        pcall(function()
            for i,v in pairs(game:service"Players":GetChildren()) do
                if not table.find(BountyControl['alts'], v.UserId) and v.Name ~= whitelistedBuyer then
                    if v.Character then
                        if (v.Character.HumanoidRootPart.Position - player.Character.HumanoidRootPart.Position).Magnitude <= 20 then
                            functions.kick(v.Name)
                        end
                    end
                end
            end
        end)
    end
end)

