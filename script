    teleportButton.MouseButton1Click:Connect(function()
        if crashModeEnabled == true then
            alert("cannot use this feature, crash mode enabled.")    
        end
        local function getAllSelectedChildren()
            local c = {}

            for i,v in pairs(AltsPanel:GetChildren()) do
                if v:IsA("Frame") then
                    if v:FindFirstChildWhichIsA("TextButton").BackgroundColor3 == green then
                        table.insert(c, v.Name:split("_")[2])
                    end
                end
            end
            return c
        end

        local newAssignment = ""
        for i,v in pairs(getAllSelectedChildren()) do
            newAssignment = newAssignment..""..tostring(functions.fetchMyId(getgenv().BountyControl["alts"],v))
        end

        functions.say(".t "..tostring(newAssignment).." "..tostring(newBuyerName))
    end)

    crashMode.MouseButton1Click:Connect(function()
        crashModeEnabled = not crashModeEnabled
        if crashModeEnabled then
            alert("crash mode enabled")
            crashMode.Text = "  crash mode | on"
            crashStart.Visible = true
        else
            crashStart.Visible = false
            crashMode.Text = "  crash mode | off"
            alert("crash mode disabled")

            for i,v in pairs(AltsPanel:GetChildren()) do
                if v:FindFirstChildWhichIsA("TextButton").BackgroundColor3 == green then
                    v:FindFirstChildWhichIsA("TextButton").BackgroundColor3 = red
                end
            end
        end
    end)

    crashStart.MouseButton1Click:Connect(function()
        local function getAllSelectedChildren()
            local c = {}

            for i,v in pairs(AltsPanel:GetChildren()) do
                if v:IsA("Frame") then
                    if v:FindFirstChildWhichIsA("TextButton").BackgroundColor3 == green then
                        table.insert(c, v.Name:split("_")[2])
                    end
                end
            end
            table.insert(c, player.UserId)
            return c
        end

        local newAssignment = ""
        for i,v in pairs(getAllSelectedChildren()) do
            newAssignment = newAssignment..""..tostring(functions.fetchMyId(getgenv().BountyControl["alts"],v))
        end

        functions.say(".ca "..tostring(newAssignment))
    end)

    nameIn.FocusLost:Connect(function()
        local out = functions.getPlayer(nameIn.Text)
        nameIn.Text = "  "..tostring(nameIn.Text)
        if out then
            newBuyerName = out.Name

            if not workspace.Players:FindFirstChild(out.Name) then
                repeat wait(0.01) until workspace.Players:FindFirstChild(out.Name)
            end

            pcall(function()
                local cash = out.DataFolder.Currency
                local wanted = out.leaderstats.Wanted

                if wanted.Value <= 0 then
                    canCollectOut.Text = "  true"
                else
                    canCollectOut.Text = "  false"
                end

                cashOut.Text = "  $"..tostring(functions.formatNum(cash.Value))
                bountyOut.Text = "  "..tostring(functions.formatNum(wanted.Value))

                cash.Changed:Connect(function()
                    cashOut.Text = "  $"..tostring(functions.formatNum(cash.Value))
                end)
                wanted.Changed:Connect(function()
                    bountyOut.Text = "  "..tostring(functions.formatNum(wanted.Value))
                end)
            end)
        else
            nameIn.Text = "  text"
        end
    end)

    local cacheSelection = {}
    local function refreshAltList()
        local function updateCacheList()
            cacheSelection = {}

            for i,v in pairs(AltsPanel:GetChildren()) do
                if v:IsA("Frame") then
                    local ind = v:FindFirstChildWhichIsA("TextButton")
                    if ind then
                        if ind.BackgroundColor3 == green then
                            table.insert(cacheSelection, v.Name:split("_")[2])
                        end
                    end
                end
            end
        end

        for i,v in pairs(AltsPanel:GetChildren()) do
            if v:IsA("Frame") then
                v:Destroy()
            end
        end

        for i,v in pairs(getgenv().BountyControl["alts"]) do
            if functions.checkPlayer(v) then
                if not workspace.Players:FindFirstChild(game:service"Players":GetPlayerByUserId(v).Name) then
                    repeat wait(0.01) until workspace.Players:FindFirstChild(game:service"Players":GetPlayerByUserId(v).Name)
                end

                if not AltsPanel:FindFirstChild("AltCardHolder_"..tostring(v)) then
                    local AltCardHolder = Instance.new("Frame")
                    local selectButton = Instance.new("TextButton")
                    local UICorner = Instance.new("UICorner")
                    local outLabel = Instance.new("TextLabel")
            
                    AltCardHolder.Name = "AltCardHolder_"..tostring(v)
                    AltCardHolder.Parent = AltsPanel
                    AltCardHolder.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    AltCardHolder.BackgroundTransparency = 1.000
                    AltCardHolder.Size = UDim2.new(0, 233, 0, 17)
                    
                    selectButton.Name = "selectButton"
                    selectButton.Parent = AltCardHolder
                    selectButton.BackgroundColor3 = red
                    selectButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    selectButton.Size = UDim2.new(0, 17, 0, 17)
                    selectButton.Font = Enum.Font.SourceSans
                    selectButton.Text = ""
                    selectButton.TextColor3 = Color3.fromRGB(0, 0, 0)
                    selectButton.TextSize = 1.000
                    
                    UICorner.CornerRadius = UDim.new(0, 3)
                    UICorner.Parent = selectButton
                    
                    outLabel.Name = "outLabel"
                    outLabel.Parent = AltCardHolder
                    outLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    outLabel.BackgroundTransparency = 1.000
                    outLabel.Position = UDim2.new(0.0987124443, 0, 0, 0)
                    outLabel.Size = UDim2.new(0, 201, 0, 17)
                    outLabel.Font = Enum.Font.SourceSans
                    outLabel.Text = "AltName - B: 0"
                    outLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                    outLabel.TextSize = 16.000
                    outLabel.TextXAlignment = Enum.TextXAlignment.Left
            
                    local altPlayer = game:service"Players":GetPlayerByUserId(v)
                    local bountyOld = altPlayer.leaderstats.Wanted.Value
                    outLabel.Text = tostring(altPlayer.Name).." - B: "..tostring(functions.formatNum(bountyOld))

                    altPlayer.leaderstats.Wanted.Changed:Connect(function()
                        outLabel.Text = tostring(altPlayer.Name).." - B: "..tostring(functions.formatNum(altPlayer.leaderstats.Wanted.Value))
                    end)
                    
                    selectButton.MouseButton1Click:Connect(function()
                        if tonumber(altPlayer.leaderstats.Wanted.Value) > 0 or crashModeEnabled == true then
                            updateCacheList()
                            if selectButton.BackgroundColor3 == red then
                                selectButton.BackgroundColor3 = green
                            else
                                selectButton.BackgroundColor3 = red
                            end
                        else
                            alert("this account has 0 bounty.")
                        end
                    end)
                end
            end
        end
    end

    refreshAltList()
    game:service"Players".PlayerAdded:Connect(function(plr)
        repeat wait(0.01) until workspace.Players:FindFirstChild(plr.Name)
        refreshAltList()
    end)
    game:service"Players".PlayerRemoving:Connect(function(plr)
        refreshAltList()
        if plr.Name == whitelistedBuyer then
            whitelistedBuyer = ""
            functions.tpHome()
        end
    end)
end
